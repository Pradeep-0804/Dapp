# Environment Variables for Testnet Deployment
# Copy this file to .env and fill in your actual values

# Your 12-word mnemonic phrase (NEVER commit this to version control!)
MNEMONIC="your twelve word mnemonic phrase goes here never share this publicly"

# Infura Project ID for Ethereum node access
# Get this from https://infura.io after creating a project
INFURA_PROJECT_ID="your_infura_project_id_here"

# Optional: Etherscan API Key for contract verification
# Get this from https://etherscan.io/apis
ETHERSCAN_API_KEY="your_etherscan_api_key_here"

# Network configurations
# NETWORK=sepolia  # Use 'sepolia' for Sepolia testnet, 'development' for local